<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fr1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/design_default_color_background"
    tools:context="com.example.lessons.FragmentList">

    <TextView
        android:id="@+id/contact1"
        android:layout_width="350dp"
        android:layout_height="140dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/style_main_frame"
        app:barrierMargin="2pt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar2" />

    <TextView
        android:id="@+id/Photo1"
        android:layout_width="98dp"
        android:layout_height="98dp"
        android:layout_marginStart="8dp"
        android:text=" Photo1"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/contact1"
        app:layout_constraintStart_toStartOf="@+id/contact1"
        app:layout_constraintTop_toTopOf="@+id/contact1"
        app:layout_constraintVertical_bias="0.19999999"
        android:background="@drawable/style_frame"/>

    <TextView
        android:id="@+id/textView11"
        android:layout_width="200dp"
        android:layout_height="64dp"
        android:text=" Name"
        app:layout_constraintEnd_toEndOf="@+id/contact1"
        app:layout_constraintHorizontal_bias="0.26"
        app:layout_constraintStart_toEndOf="@+id/Photo1"
        app:layout_constraintTop_toTopOf="@+id/Photo1"
        android:background="@drawable/style_frame"/>

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar2"
        android:layout_width="410dp"
        android:layout_height="wrap_content"
        android:background="#90d0cc"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="List" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:background="@drawable/style_frame"
        android:text=" Number"
        app:layout_constraintBottom_toBottomOf="@+id/Photo1"
        app:layout_constraintStart_toStartOf="@+id/textView11" />


</androidx.constraintlayout.widget.ConstraintLayout>
